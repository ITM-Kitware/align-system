    "version": "0.2.0",
    "configurations": [
        {
            "name": "High Incontext Adept",
            "type": "debugpy",
            "request": "launch",
            "console": "integratedTerminal",
            "module": "align_system.cli.run_align_system",
            "args": [
                    "loglevel=\"DEBUG\"",
                    "+experiment=metrics_refinement_evaluation/single_kdma_aligned_adept_high",
                    "adm=incontext/moral_deservingness",
                    "adm.inference_kwargs.n_negative_samples=0",
                    "interface.api_endpoint='http://127.0.0.1:8080'",
                    ],
            "env":  {
                    "CUDA_VISIBLE_DEVICES": "1"
                    }
        },
